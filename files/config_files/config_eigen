server {
	server_name webserv
	listen 8000

	root files/response_files/
	index index.html

	allow_methods GET

	location / {

	}
	location /put_test {
		index put_test.html
		allow_methods GET
	}

	location /foldertest {
		index test.html
	}

	location /foldertest/dubbeletest {
		index test.html
	}

	location /delete_test {
		index delete.html
		allow_methods POST DELETE
		client_body_buffer_ size 100
	}

	location /directory {
		alias
		allow_methods GET
		root YoupiBanane/
		index youpi.bad_extension
	}

	location /test {
		allow_methods DELETE
		cgi_pass test_linux/ubuntu_cgi_tester
	}
}